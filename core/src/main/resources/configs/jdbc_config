jdbcdriver=org.postgresql.Driver
url=jdbc:postgresql://localhost:5432/jetty
username=postgres
password=admin

usertable=security.users
usertablekey=id
usertableuserfield=name
usertablepasswordfield=password

roletable=security.roles
roletablekey=id
roletablerolefield=role

userroletable=security.users_roles
userroletableuserkey=user_id
userroletablerolekey=role_id